{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11"
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/uploads/(.*)", "dest": "/api/index.py" },
    { "src": "/healthz", "dest": "/api/index.py" },
    { "src": "/login(.*)", "dest": "/api/index.py" },
    { "src": "/templates/(.*)", "dest": "/api/index.py" }
  ]
}